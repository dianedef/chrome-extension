{
    "update_url": "https://clients2.google.com/service/update2/crx",
    "manifest_version": 2,
    "name": "Outliner | A Chrome Extension for Outline.com",
    "version": "1.4.4",
    "description": "Tired of COPYing and PASTEing URL's in Outline.com website, I created an extension",
    "author": "Rodrigo Vasconcellos",
    "icons": {
        "16": "outline-logo-16.png",
        "48": "outline-logo-48.png",
        "128": "outline-logo.png"
    },
    "browser_action": {
        "default_icon": "outline-logo.png",
        "default_title": "Outliner"
    },
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
        },
    "options_ui": {
    "page": "options.html",
    "open_in_tab": false
    },
    "permissions": [
        "activeTab",
        "contextMenus",
        "storage"
    ]
}


{
    "name": "My extension",
    "commands": {
        "toggle-feature-foo": {
            "suggested_key": {
                "default": "Ctrl+Shift+Y",
                "mac": "Command+Shift+Y"
            },
            "description": "Toggle feature foo",
            "global": true
        }
    }
}

